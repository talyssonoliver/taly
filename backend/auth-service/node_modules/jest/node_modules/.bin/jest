#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/jest-cli@27.5.1_node-notifier@8.0.2/node_modules/jest-cli/bin/node_modules:/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/jest-cli@27.5.1_node-notifier@8.0.2/node_modules/jest-cli/node_modules:/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/jest-cli@27.5.1_node-notifier@8.0.2/node_modules:/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/jest-cli@27.5.1_node-notifier@8.0.2/node_modules/jest-cli/bin/node_modules:/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/jest-cli@27.5.1_node-notifier@8.0.2/node_modules/jest-cli/node_modules:/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/jest-cli@27.5.1_node-notifier@8.0.2/node_modules:/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../jest-cli/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../jest-cli/bin/jest.js" "$@"
fi
