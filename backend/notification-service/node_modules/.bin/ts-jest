#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.26.7_babel-jest@27.5.1_@babel+core@7.26.7__jest@27.5.1_typescript@4.9.5/node_modules/ts-jest/node_modules:/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.26.7_babel-jest@27.5.1_@babel+core@7.26.7__jest@27.5.1_typescript@4.9.5/node_modules:/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.26.7_babel-jest@27.5.1_@babel+core@7.26.7__jest@27.5.1_typescript@4.9.5/node_modules/ts-jest/node_modules:/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.26.7_babel-jest@27.5.1_@babel+core@7.26.7__jest@27.5.1_typescript@4.9.5/node_modules:/mnt/c/taly/dir-taly/taly/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
